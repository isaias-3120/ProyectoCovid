{
  "expo": {
    "name": "COVID Tracking APP",
    "slug": "proyectoreact",
    "description":"Aplicacion para checar las estadisticas mas recientes acerca del COVID",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/coviIco.png",
    "splash": {
      "image": "./assets/coviSP.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
